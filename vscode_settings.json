{
    "explorer.confirmDelete": false, 
    "editor.fontFamily": "UbuntuMono Nerd Font Mono",
    //"editor.fontFamily": "CodeNewRoman Nerd Font",
    "editor.fontSize": 15,
    "editor.fontLigatures": true,
    "editor.smoothScrolling": true,
    "diffEditor.codeLens": true,
    "editor.minimap.scale": 1,
    "telemetry.telemetryLevel": "off",
    "editor.bracketPairColorization.enabled": true,
    "vim.smartRelativeLine": true,
    "terminal.integrated.fontSize": 13.5,
    "editor.semanticHighlighting.enabled": false,
    "workbench.iconTheme": "vscode-icons",
    "workbench.fontAliasing": "auto",
    "debug.console.fontSize": 13.5,
    "explorer.confirmDragAndDrop": false,
    "vsicons.dontShowNewVersionMessage": true,
    "editor.linkedEditing": true,
    "editor.detectIndentation": true,
    "editor.lineHeight": 0,
    //"editor.inlineSuggest.enabled": true,
    "workbench.sideBar.location": "left",
    "explorer.decorations.colors": false,
    "workbench.editor.enablePreview": true,
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "window.zoomPerWindow": true,
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "workbench.colorCustomizations": {
        "editor.selectionBackground": "#575F69",
        "editor.findMatchHighlightBorder": "#575F69",

        "[*Dark*]":{
            "editorBracketHighlight.foreground1": "#f9f9fa",
            "editorBracketHighlight.foreground2": "#fed141",
            "editorBracketHighlight.foreground3": "#f9f9fa",
            "editorBracketHighlight.foreground4": "#f9f9fa",
            "editorBracketHighlight.foreground5": "#fed141",
            "editorBracketHighlight.foreground6": "#f9f9fa",
        },
        "[*Dimmed*]":{
            "editorBracketHighlight.foreground1": "#f9f9fa",
            "editorBracketHighlight.foreground2": "#fed141",
            "editorBracketHighlight.foreground3": "#f9f9fa",
            "editorBracketHighlight.foreground4": "#f9f9fa",
            "editorBracketHighlight.foreground5": "#fed141",
            "editorBracketHighlight.foreground6": "#f9f9fa",
        },
        "[*Light*]":{
            "editorBracketHighlight.foreground1": "#2b3036",
            "editorBracketHighlight.foreground2": "#4986EC",
            "editorBracketHighlight.foreground3": "#2b3036",
            "editorBracketHighlight.foreground4": "#2b3036",
            "editorBracketHighlight.foreground5": "#4986EC",
            "editorBracketHighlight.foreground6": "#2b3036",
        },

        "focusBorder": "#2B2D2F",
        "statusBar.background": "#2b3036",
        "statusBarItem.hoverBackground": "#266ce3",
        "statusBar.border": "#2b3036",
        //"editor.hoverHighlightBackground": "#0000ff",
        "scrollbar.shadow": "#2b3036",
        "scrollbarSlider.background": "#2b3036",
        "scrollbarSlider.activeBackground": "#0000ff", 
        "tab.activeBorderTop": "#707681",
        "activityBar.activeBorder": "#707681",
        "activityBar.activeBackground": "#2b3036",
        "list.warningForeground": "#d77064",
        "gitDecoration.modifiedResourceForeground": "#d77064",
        "gitDecoration.untrackedResourceForeground": "#d77064",
        "input.background": "#0000ff",
        "input.foreground": "#fbf9f9",
        //"input.placeholderForeground": "#fbf9f9",
        "button.background": "#0000ff",
        "button.hoverBackground" : "#4985ec",
        "sideBar.background": "#24292F",
        "editor.findMatchHighlightBackground": "#282D31",
        "editor.findMatchBackground": "#282D31",
        "editor.findMatchBorder":"#282D31", 
        "searchEditor.findMatchBorder": "#586069",
        "breadcrumb.background": "#202428",
        "tab.activeForeground": "#fbf9f9",
        "tab.activeBackground": "#202428",
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "entity.name.function.java",
                    "constant.language.json",
                    "variable",
                    "punctuation.accessor.cs",
                    "source.cs",
                    "entity.name.function.cs",
                    "entity.name.type.namespace.cs",
                    "punctuation.parenthesis.close.cs",
                    "punctuation.parenthesis.open.cs",
                    "entity.name.variable.property.cs",
                    "entity.name.type.class.cs",
                    "punctuation.definition.string.end.powershell",
                    "punctuation.definition.string.begin.powershell",
                ],
                "settings": {
                    "foreground": "#fbf9f9",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": [
                    "support.type.property-name",
                    "variable.other.member.powershell",
                ],
                "settings": {
                    "foreground": "#4EC9B0"
                }
            },
            {
                "scope": [
                    "constant.numeric",
                    "support.function.builtin.shell",
                ],
                "settings": {
                    "foreground": "#ff6a80",
                    //"foreground": "#feea7f",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": [
                    "string.quoted.double.json.comments",
                    "string.quoted.single.js",
                    "string.quoted.double.java",
                    "string.quoted.double.json",
                    "string.quoted.single.python",
                ],
                "settings": {
                    //"foreground": "#598eed",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": [
                    "comment.line.double-slash.js",
                    "comment.line.number-sign.python",
                    "comment.line.powershell",
                    "comment.block.powershell",
                ],
                "settings": {
                    "foreground": "#575e69"
                }
            },
            {
                "scope": [
                    "comment.block.javadoc.java",
                    "punctuation.definition.comment.java",
                    "string.quoted.double.json.comments"
                ],
                "settings": {
                    "foreground": "#4985ec"
                }
            },
            {
                "scope": [
                    "storage.modifier.cs",
                    "keyword.other.class.cs",
                    "keyword.other.namespace.cs",
                    "keyword.other.using.cs",
                    "keyword.control.flow.return.cs",
                    "variable.other.readwrite.cs",
                    "keyword.other.base.cs",
                    "keyword.control.loop.foreach.cs",
                    "keyword.other.var.cs",
                    "keyword.control.conditional.if.cs",
                    "constant.language.null.cs",
                    "keyword.type.cs",
                    "punctuation.definition.string.begin.cs",
                    "punctuation.definition.string.end.cs",
                    "keyword.control.loop.in.cs",
                    "keyword.other.this.cs",
                    "keyword.operator.comparison.cs",
                    "keyword.control.conditional.else.cs",
                    "keyword.control.cs",
                    "keyword.operator.assignment.compound.bitwise.cs",
                    "keyword.operator.bitwise.cs",
                    "punctuation.definition.string.end.shell",
                    "punctuation.definition.string.begin.shell",
                    "keyword.control.loop.while.cs",
                    "keyword.control.try.cs",
                    "keyword.control.flow.break.cs",
                    "keyword.control.try.catch.cs",
                    "keyword.control.flow.throw.cs",
                ],
                "settings": {
                    "foreground": "#fed141"
                }
            },
            {
                "scope": [
                    "variable.other.readwrite.cs",
                    "keyword.other.get.cs",
                    "keyword.other.set.cs",
                    "meta.function-call.generic.python",
                    "log.error",
                    "support.function.magic.python",
                    "entity.name.function.python",
                    "support.function.builtin.python",
                    "storage.type.powershell",
                    "support.function.builtin.shell",
                    "entity.name.variable.local.cs",

                    "entity.name.type.class.python",
                    "variable.legacy.builtin.python",
                    "variable.parameter.attribute.powershell",
                    "entity.name.command.shell",
                ],
                "settings": {
                    //"foreground": "#B392F0"
                    "foreground": "#007dff"
                }
            },
            {
                "scope": [
                    "entity.name.type.cs",


                ],
                "settings": {
                    "foreground": "#dabaff"
                }
            },
            {
                "scope": [
                    "keyword.control.flow.python",
                ],
                "settings": {
                    "foreground": "#007dff"
                }
            },
            {
                "scope": [
                    "log.debug"
                ],
                "settings": {
                    "foreground": "#5beb95"
                }
            },
            {
                "scope": [
                    "variable.parameter.function-call.python",
                    "entity.name.variable.local.cs",
                    "entity.name.variable.field.cs",
                    "keyword.operator.hl7",
                    "constant.character.hl7"
                ],
                "settings": {
                    "foreground": "#ff6a80"
                }
            },
            {
                "scope": [
                    "entity.name.variable.local.cs",
                    "entity.name.variable.field.cs",
                    "variable.other.object.cs",
                    "variable.other.readwrite.cs",
                ],
                "settings": {
                    "foreground": "#4fb0cc"
                }
            },
            {
                "scope": [

                    "constant.character.escape.json",
                ],
                "settings": {
                    "foreground": "#d8ba71"
                }
            },
            {
                "scope": [
                    //"variable.other.readwrite.powershell",
                    //"variable.other.normal.shell",

                ],
                "settings": {
                    "foreground": "#000000",
                    "fontStyle": "bold",
                }
            }
        ]
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "workbench.editorAssociations": {
        "*.dll": "default"
    },
    "files.associations": {
        "*.json": "jsonc"
    },
    "editor.inlineSuggest.suppressSuggestions": true,
    "workbench.colorTheme": "GitHub Dark",
}
