{
    "explorer.confirmDelete": false,
	"editor.fontFamily": "Ubuntu Mono",
    "editor.fontSize": 14,
    "editor.fontLigatures": true,
    "editor.smoothScrolling": true,
    "diffEditor.codeLens": true,
    "editor.minimap.scale": 1,
    "telemetry.telemetryLevel": "off",
    "editor.bracketPairColorization.enabled": true,
    "vim.smartRelativeLine": true,
    "terminal.integrated.fontSize": 14,
    "editor.semanticHighlighting.enabled": false,
    "workbench.colorCustomizations": {
        "editorBracketHighlight.foreground1": "#f9f9fa",
        "editorBracketHighlight.foreground2": "#f9f9fa",
        "editorBracketHighlight.foreground3": "#f9f9fa",
        "editorBracketHighlight.foreground4": "#f9f9fa",
        "editorBracketHighlight.foreground5": "#f9f9fa",
        "editorBracketHighlight.unexpectedBracket.foreground": "#fdfcfc",
        "statusBar.background": "#266ce3",
        "tab.activeBorderTop": "#266ce3",
        "activityBar.activeBorder": "#266ce3",
        "list.warningForeground": "#d77064",
        "gitDecoration.modifiedResourceForeground": "#d77064",
        "gitDecoration.untrackedResourceForeground": "#d77064",
    },
    "workbench.editor.enablePreview": true,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "entity.name.function.java",
                    "constant.language.json",
                    "variable",
                    "variable.other.object.cs",
                    "punctuation.accessor.cs",
                    "source.cs",
                    "entity.name.function.cs",
                    "entity.name.type.namespace.cs",
                    "punctuation.parenthesis.close.cs",
                    "punctuation.parenthesis.open.cs",
                    "entity.name.variable.property.cs",
                    "entity.name.type.class.cs",
                ],
                "settings": {
                    "foreground": "#fbf9f9",
                },
            },
            {
                "scope": [
                    "entity.name.type.cs",
                ],
                "settings": {
                    "foreground": "#266ce3",
                    "fontStyle": "bold",
                },
            },
            {
                "scope": [
                    "support.type.property-name",
                ],
                "settings": {
                    "foreground": "#4EC9B0",
                },
            },
            {
                "scope" : [
                    "constant.numeric",
                    "string.quoted.double.cs",
                ],
                "settings": {
                    "foreground": "#c0da4e",
                    "fontStyle": "bold"
                }
            },
            {
                "scope" : [
                    "string.quoted.double.json.comments",
                    "string.quoted.single.js",
                    "string.quoted.double.java",
                    "string.quoted.double.json",
                    "variable.other.object.cs",
                    "string.quoted.single.python",
                    "keyword.type.cs",
                ],
                "settings": {
                    "foreground": "#266ce3",
                    "fontStyle": "bold",
                },
            },
            {
                "scope" : [
                    "comment.line.double-slash.js",
                    "comment.line.number-sign.python",
                ],
                "settings": {
                    "foreground": "#575e69",
                },
            },
            {
                "scope" : [
                    "comment.block.javadoc.java",
                    "punctuation.definition.comment.java",
                ],
                "settings": {
                    "foreground": "#4985ec",
                }
            },
            {
                "scope" : [
                    "storage.modifier.cs",
                    "keyword.other.class.cs",
                    "keyword.other.namespace.cs",
                    "keyword.other.using.cs",
                    "keyword.control.flow.return.cs",
                    "variable.other.readwrite.cs",
                    "keyword.type.cs",
                    "keyword.other.base.cs",
                    "keyword.control.loop.foreach.cs",
                    "keyword.other.var.cs",
                    "keyword.control.conditional.if.cs",
                    "constant.language.null.cs",
                    "keyword.type.cs",
                    "punctuation.definition.string.begin.cs",
                    "punctuation.definition.string.end.cs",
                    "keyword.control.loop.in.cs",
                    "keyword.other.this.cs",
                    "keyword.operator.comparison.cs",
                    "keyword.control.conditional.else.cs",
                    "keyword.control.cs",
                    "keyword.operator.assignment.compound.bitwise.cs",
                    "keyword.operator.bitwise.cs",
                ],
                "settings": {
                    "foreground": "#fe7ab2",
                }
            },
            {
                "scope" : [
                    "variable.other.readwrite.cs",
                    "keyword.other.get.cs",
                    "keyword.other.set.cs",
                    "meta.function-call.generic.python",
                    "log.error",
                ],
                "settings": {
                    "foreground": "#B392F0",
                }
            },
            {
                "scope" : [
                    "keyword.type.cs",
                    "entity.name.type.cs",
                    "variable.other.object.cs",
                ],
                "settings": {
                    "foreground": "#006eec",
                }
            },
            {
                "scope" : [
                    "log.debug",
                ],
                "settings": {
                    "foreground": "#5beb95",
                }
            },
            {
                "scope" : [
                   	"variable.parameter.function-call.python",
                    "variable.parameter.function.language.python",
                ],
                "settings": {
                    "foreground": "#ff6a80", 
                }
            },
            {
                "scope" : [
                ],
                "settings": {
                    "foreground": "#83a0bf",
                }
            },
        ]
    },
    "workbench.colorTheme": "GitHub Dark",
    "workbench.iconTheme": "vscode-icons",
    "debug.console.fontSize": 14,
    "remoteHub.commitDirectlyWarning": "off",
    "window.zoomLevel": 1,
    "explorer.confirmDragAndDrop": false,
}